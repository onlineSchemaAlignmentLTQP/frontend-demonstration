@prefix semmap: <https://onlineschemaalignmentltqp.github.io/vocabulary/vocab.ttl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix snvoc: <http://localhost:4099/www.ldbc.eu/ldbc_socialnet/1.0/vocabulary/> .
@prefix snvocHttps: <https://localhost:4099/www.ldbc.eu/ldbc_socialnet/1.0/vocabulary/> .

[]
    a semmap:RuleSet ;
    semmap:subweb "*" ;
    semmap:rule
        [
            semmap:premise snvoc:likes ;
            semmap:inference skos:exactMatch ;
            semmap:conclusion snvocHttps:likes
        ],
        [
            semmap:premise snvoc:hasPost ;
            semmap:inference skos:exactMatch ;
            semmap:conclusion snvocHttps:hasPost
        ],
        [
            semmap:premise snvoc:hasComment ;
            semmap:inference skos:exactMatch ;
            semmap:conclusion snvocHttps:hasComment
        ] .
